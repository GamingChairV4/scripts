	-- Scripts:

	local function JTUDQ_fake_script() -- killAura.auraLocal 
		local script = Instance.new('LocalScript', killAura)

		local aura = script.Parent

		aura.MouseButton1Click:Connect(function()
			-- kill aura

			local Players = game:GetService("Players")
			local ReplicatedStorage = game:GetService("ReplicatedStorage")
			local DefaultChatSystemChatEvents = ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents")
			local LocalPlayer = Players.LocalPlayer

			local messages = {
				"My gaming chair is too good ",
				"On the warlord grind ",
				"Im using AsuraV4 ",

			}

			local previousTargets = {}

			local function getCurrentHeldSword(character)
				for _, item in pairs(character:GetChildren()) do
					if item:IsA("Tool") and item.Name:lower():find("sword") then
						return item.Name
					end
				end
				return nil
			end

			local function checkPlayersInRange()
				while task.wait() do
					local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
					if Character and Character:FindFirstChild("HumanoidRootPart") then
						local localPlayerPos = Character.HumanoidRootPart.Position
						local currentSword = getCurrentHeldSword(Character)

						if currentSword then
							for _, player in pairs(Players:GetPlayers()) do
								if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
									local playerPos = player.Character.HumanoidRootPart.Position
									local distance = (localPlayerPos - playerPos).Magnitude

									if distance <= 50 then
										local direction = (playerPos - localPlayerPos).unit

										local args = {
											[1] = player.Character,
											[2] = direction,
											[3] = currentSword
										}
										if player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.Health > 0 then
											ReplicatedStorage.Remotes.SwordAttack:InvokeServer(unpack(args))
											previousTargets[player] = nil

										elseif player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.Health <= 0 and not previousTargets[player] then
											local randomMessage = messages[math.random(#messages)] .. " " .. player.Name
											local chatArgs = {
												[1] = randomMessage,
												[2] = "All"
											}
											DefaultChatSystemChatEvents:WaitForChild("SayMessageRequest"):FireServer(unpack(chatArgs))
											previousTargets[player] = true
										end
									end
								end
							end
						end
					end
				end
			end

			local function waitForSwordAndCheck()
				while true do
					if getCurrentHeldSword(LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()) then
						checkPlayersInRange()
					end
					task.wait()
				end
			end

			LocalPlayer.CharacterAdded:Connect(function(character)
				character:WaitForChild("HumanoidRootPart")
				waitForSwordAndCheck()
			end)

			waitForSwordAndCheck()
		end)
	end
	coroutine.wrap(JTUDQ_fake_script)()
	local function SPAPLLJ_fake_script() -- bedTp.LocalScript 
		local script = Instance.new('LocalScript', bedTp)

		local bedTP = script.Parent

		bedTP.MouseButton1Click:Connect(function()
			local player = game.Players.LocalPlayer
			local beds = game.Workspace.Map.Beds:GetChildren()

			local function teleportToBeds()
				for _, bed in ipairs(beds) do
					if bed:FindFirstChild("Mattress") and bed.Mattress.BrickColor ~= player.TeamColor then
						player.Character.HumanoidRootPart.CFrame = (CFrame.new(bed.Mattress.Position))
						wait(5)
					end
				end
			end

			teleportToBeds()
		end)

	end
	coroutine.wrap(SPAPLLJ_fake_script)()
	local function UBJB_fake_script() -- bedNuker.nukerLocal 
		local script = Instance.new('LocalScript', bedNuker)

		local Players = game:GetService("Players")
		local ReplicatedStorage = game:GetService("ReplicatedStorage")
		local LocalPlayer = Players.LocalPlayer

		local bedNukerEnabled = false -- Initial state of bed nuker (disabled)

		local function autoDamageBedsFunction()
			while bedNukerEnabled do
				local playerPosition = LocalPlayer.Character and LocalPlayer.Character.PrimaryPart and LocalPlayer.Character.PrimaryPart.Position
				if playerPosition then
					local bedsFolder = workspace:WaitForChild("Map"):WaitForChild("Beds")
					local woodPick = "Wooden Pickaxe"
					local stonePick = "Stone Pickaxe"
					local ironPick = "Iron Pickaxe"
					local dimPick = "Diamond Pickaxe"
					local woodAxe = "Wooden Axe"
					local stoneAxe = "Stone Axe"
					local ironAxe = "Iron Axe"
					local dimAxe = "Diamond Axe"

					local remote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("DamageBlock")

					local function getNearestBed()
						local nearestBed = nil
						local shortestDistance = math.huge
						local playerTeamColor = LocalPlayer.TeamColor

						for _, bed in ipairs(bedsFolder:GetChildren()) do
							if bed:IsA("Model") and bed:FindFirstChild("Primary") and bed:FindFirstChild("Mattress") then
								local mattress = bed.Mattress
								if mattress.BrickColor == playerTeamColor then
									continue
								end

								local bedPosition = bed.Primary.Position
								local distance = (bedPosition - playerPosition).Magnitude

								if distance < shortestDistance then
									nearestBed = bed
									shortestDistance = distance
								end
							end
						end

						return nearestBed
					end

					local nearestBed = getNearestBed()
					if nearestBed then
						local args = {
							[1] = nearestBed,
							[2] = woodPick
	--[[                [3] = woodAxe
	                    [4] = stoneAxe
	                    [5] = ironAxe
	                    [6] = dimAxe
	                    [7] = stonePick
	                    [8] = ironAxe
	                    [9] = dimPick]]
						}
						remote:InvokeServer(unpack(args))
					end
				end
				task.wait()
			end
		end

		local function toggleBedNuker()
			bedNukerEnabled = not bedNukerEnabled
			if bedNukerEnabled then
				spawn(autoDamageBedsFunction)
			end

			-- Update the button text
			local button = script.Parent
			if button and button:IsA("TextButton") then
				if bedNukerEnabled then
					button.Text = "Bed Nuker: On"
				else
					button.Text = "Bed Nuker: Off"
				end
			end
		end

		-- Connect the button click to the toggle function
		local button = script.Parent
		if button and button:IsA("TextButton") then
			button.MouseButton1Click:Connect(toggleBedNuker)
		end
	end
	coroutine.wrap(UBJB_fake_script)()
	local function HKUWIZ_fake_script() -- playerTp.tpLocal 
		local script = Instance.new('LocalScript', playerTp)

		local teleportEnabled = false 
		local player = game.Players.LocalPlayer

		local function getValidPlayers()
			local validPlayers = {}
			for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
				if otherPlayer ~= player and otherPlayer.Team ~= player.Team and otherPlayer.Team.Name ~= "Spectator" then
					table.insert(validPlayers, otherPlayer)
				end
			end
			return validPlayers
		end

		local function teleportToPlayers()
			local validPlayers = getValidPlayers()
			if #validPlayers == 0 then return end

			local currentPlayerIndex = 1

			while teleportEnabled do
				local targetPlayer = validPlayers[currentPlayerIndex]
				if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
					local humanoid = targetPlayer.Character:FindFirstChild("Humanoid")
					if humanoid.Health > 0 then
						player.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
						wait(0.5)
					else
						currentPlayerIndex = currentPlayerIndex % #validPlayers + 1
					end
				else
					currentPlayerIndex = currentPlayerIndex % #validPlayers + 1
				end
			end
		end

		local function toggleTeleport()
			teleportEnabled = not teleportEnabled
			if teleportEnabled then
				teleportToPlayers()
				script.Parent.Text = "Player Teleport: On"
			else
				script.Parent.Text = "Player Teleport: Off"
			end
		end

	end
	coroutine.wrap(HKUWIZ_fake_script)()
