		end)
	end
	coroutine.wrap(MJNXR_fake_script)()
	local function TFVA_fake_script() -- ScreenGui.Bind 
		local script = Instance.new('LocalScript', ScreenGui)

		local player = game.Players.LocalPlayer
		local userInputService = game:GetService("UserInputService")


		local gui = script.Parent

		local function toggleGui()
			gui.Enabled = not gui.Enabled
		end

		userInputService.InputBegan:Connect(function(input)
			if input.KeyCode == Enum.KeyCode.RightShift then
				toggleGui()
			end
		end)

	end
	coroutine.wrap(TFVA_fake_script)()
	local function VZTIBA_fake_script() -- autoLoot.autoLootLocal 
		local script = Instance.new('LocalScript', autoLoot)

		local autoLoot = script.Parent

		autoLoot.MouseButton1Up:Connect(function()
			local screenGui = Instance.new("ScreenGui")
			screenGui.Name = "TeleportationGUI"
			screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
			screenGui.ResetOnSpawn = false

			local frame = Instance.new("Frame")
			frame.Name = "ControlFrame"
			frame.Size = UDim2.new(0, 200, 0, 150)
			frame.Position = UDim2.new(0.5, -100, 0.5, -75)
			frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			frame.BackgroundTransparency = 0.5
			frame.Active = true
			frame.Draggable = true
			frame.Parent = screenGui

			local closeButton = Instance.new("TextButton")
			closeButton.Name = "CloseButton"
			closeButton.Size = UDim2.new(0, 20, 0, 20)
			closeButton.Position = UDim2.new(1, -25, 0, 5)
			closeButton.Text = "X"
			closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
			closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			closeButton.Parent = frame

			local toggleIron = Instance.new("TextButton")
			toggleIron.Name = "ToggleIron"
			toggleIron.Size = UDim2.new(1, 0, 0.25, 0)
			toggleIron.Position = UDim2.new(0, 0, 0, 0)
			toggleIron.Text = "Teleport Iron"
			toggleIron.TextColor3 = Color3.fromRGB(255, 255, 255)
			toggleIron.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
			toggleIron.Parent = frame

			local toggleEmerald = Instance.new("TextButton")
			toggleEmerald.Name = "ToggleEmerald"
			toggleEmerald.Size = UDim2.new(1, 0, 0.25, 0)
			toggleEmerald.Position = UDim2.new(0, 0, 0.25, 0)
			toggleEmerald.Text = "Teleport Emerald"
			toggleEmerald.TextColor3 = Color3.fromRGB(255, 255, 255)
			toggleEmerald.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			toggleEmerald.Parent = frame

			local toggleDiamond = Instance.new("TextButton")
			toggleDiamond.Name = "ToggleDiamond"
			toggleDiamond.Size = UDim2.new(1, 0, 0.25, 0)
			toggleDiamond.Position = UDim2.new(0, 0, 0.5, 0)
			toggleDiamond.Text = "Teleport Diamond"
			toggleDiamond.TextColor3 = Color3.fromRGB(255, 255, 255)
			toggleDiamond.BackgroundColor3 = Color3.fromRGB(0, 0, 255)
			toggleDiamond.Parent = frame

			local statusLabel = Instance.new("TextLabel")
			statusLabel.Name = "StatusLabel"
			statusLabel.Size = UDim2.new(1, 0, 0.25, 0)
			statusLabel.Position = UDim2.new(0, 0, 0.75, 0)
			statusLabel.Text = "Status: Idle"
			statusLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
			statusLabel.BackgroundTransparency = 1
			statusLabel.Parent = frame

			local isTeleporting = {
				Iron = false,
				Emerald = false,
				Diamond = false
			}

			local function teleportPlayerToPart(part)
				local player = game.Players.LocalPlayer
				local character = player.Character or player.CharacterAdded:Wait()
				local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")

				if humanoidRootPart then
					humanoidRootPart.CFrame = part.CFrame
				else
					warn("HumanoidRootPart not found in player's character.")
				end
			end

			local function continuouslyTeleportToItems(itemType)
				local pickupableItemsFolder = game.Workspace:FindFirstChild("PickupableItems")
				if pickupableItemsFolder then
					while isTeleporting[itemType] do
						local models = pickupableItemsFolder:GetChildren()
						if #models > 0 then
							for _, model in ipairs(models) do
								if model:IsA("Model") and model.Name == itemType then
									local basePart = model:FindFirstChild(itemType)
									if basePart and basePart:IsA("BasePart") then
										statusLabel.Text = "Collecting: " .. basePart.Name
										teleportPlayerToPart(basePart)
										wait(0.5)
									end
								end
							end
						end
						wait(0.5)
					end
					statusLabel.Text = "Status: Idle"
				else
					warn("Pickup folder not found in Workspace.")
				end
			end

			toggleIron.MouseButton1Click:Connect(function()
				isTeleporting.Iron = not isTeleporting.Iron
				toggleIron.Text = isTeleporting.Iron and "Stop Teleport Iron" or "Teleport Iron"
				if isTeleporting.Iron then
					continuouslyTeleportToItems("Iron")
				end
			end)

			toggleEmerald.MouseButton1Click:Connect(function()
				isTeleporting.Emerald = not isTeleporting.Emerald
				toggleEmerald.Text = isTeleporting.Emerald and "Stop Teleport Emerald" or "Teleport Emerald"
				if isTeleporting.Emerald then
					continuouslyTeleportToItems("Emerald")
				end
			end)

			toggleDiamond.MouseButton1Click:Connect(function()
				isTeleporting.Diamond = not isTeleporting.Diamond
				toggleDiamond.Text = isTeleporting.Diamond and "Stop Teleport Diamond" or "Teleport Diamond"
				if isTeleporting.Diamond then
					continuouslyTeleportToItems("Diamond")
				end
			end)

			closeButton.MouseButton1Click:Connect(function()
				screenGui.Enabled = false
			end)

		end)
	end
	coroutine.wrap(VZTIBA_fake_script)()
	local function LKLXQB_fake_script() -- staffDetector.staffDetectorLocal 
		local script = Instance.new('LocalScript', staffDetector)

		local staffDetector = script.Parent

		staffDetector.MouseButton1Up:Connect(function()
			local Players = game:GetService("Players")
			local TeleportService = game:GetService("TeleportService")
			local PlaceId = game.PlaceId
			local StarterGui = game:GetService("StarterGui")

			local AdminsFolder = game.ReplicatedStorage.Admins
			local lastKickTimes = {}
			local cooldownTime = 15

			local function checkAndKick()
				for _, player in ipairs(Players:GetPlayers()) do
					for _, admin in ipairs(AdminsFolder:GetChildren()) do
						if string.lower(player.Name) == string.lower(admin.Name) then
							local currentTime = tick()
							local lastKickTime = lastKickTimes[string.lower(player.Name)] or 0

							if currentTime - lastKickTime >= cooldownTime then
								-- local kickReason = "Staff Detected: (@" .. string.lower(player.Name) .. "), " .. string.lower(player.DisplayName)
								local kickReason_notLowered = "Staff Detected: (@" .. player.Name .. "), " .. player.DisplayName

								StarterGui:SetCore("SendNotification", {
									Title = "Staff";
									Text = kickReason_notLowered;
									Duration = 6;
								})

								-- Players.LocalPlayer:Kick(kickReason .. "\nJoining a new server in a few seconds.")

								wait(5)


								-- TeleportService:Teleport(PlaceId, Players.LocalPlayer)

								-- lastKickTimes[string.lower(player.Name)] = currentTime 
							end
						end
					end
				end
			end

			while true do
				checkAndKick()
				wait(0.1)
			end
		end)
	end
	coroutine.wrap(LKLXQB_fake_script)()
	
	ScreenGui.ResetOnSpawn = false
end
